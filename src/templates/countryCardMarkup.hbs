{{#each this}}
<article class="card">
  <h2 class="card__title">{{name}}</h2>
    <img class="card__img" src="{{flag}}" alt="{{name}}">
  <p class="card__text">
    <span>Capital:</span>{{capital}}
  </p>
  <p class="card__text">
    <span>Population:</span>{{population}}
  </p>
  <p class="card__text">
    <span>Languages:</span>{{capital}}
  </p>
  <ul class="card__list-lang">
    {{#languages}}
    <li class="card__item"></li>
  </ul>
</article>
{{/each}}